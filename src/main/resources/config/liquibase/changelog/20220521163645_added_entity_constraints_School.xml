<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity School.
    -->
    <changeSet id="20220521163645-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="every_school_id"
                                 baseTableName="school"
                                 constraintName="fk_school__every_school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="canteen_user"/>

        <addForeignKeyConstraint baseColumnNames="school_id"
                                 baseTableName="school"
                                 constraintName="fk_school__school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="canteen_user"/>

        <addForeignKeyConstraint baseColumnNames="schools_id"
                                 baseTableName="school"
                                 constraintName="fk_school__schools_id"
                                 referencedColumnNames="id"
                                 referencedTableName="canteen_user"/>
    </changeSet>
</databaseChangeLog>
